{
  "database": {
    "default": "blog"
  },
  "entity": {
    "category": {
      "model": {
        "CategoryID": "Number",
        "Description": "String",
        "CategoryName": "String"
      },
      "options": {
        "collection": "Categories",
        "autoIncrement": {
            "field": "CategoryID",
            "startValue": 0,
            "step": 2
        }
      }
    },
    "customer": {
      "model": {
        "City": "String",
        "Fax": "String",
        "PostalCode": "String",
        "ContactTitle": "String",
        "Phone": "String",
        "ContactName": "String",
        "CustomerID": "String",
        "Country": "String",
        "CompanyName": "String",
        "Region": "String",
        "Address": "String"
      },
      "options": {
        "collection": "Customers"
      }
    }
  }
}
