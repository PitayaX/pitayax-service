{
  "entity": {
    "anonymous": true,
    "basePath": "/api",
    "adapter": "entityAdapter",
    "router": "./entityRouter",
    "route":{
      "get": [
        { "key":"/list", "value": "list" },
        { "key":"/:id", "value": "get" },
        { "key":"/", "value": "disable" }
      ],
      "post": [
        { "key": "/script/:name", "value": "script" },
        { "key": "/create", "value": "create" },
        { "key": "/query", "value": "list" },
        { "key": "/count", "value": "count" },
        { "key":"/", "value": "create" }
      ],
      "put": [
        { "key":"/:id", "value": "update" },
        { "key":"/", "value": "update" }
      ],
      "delete": [
        { "key":"/:id", "value": "delete" },
        { "key":"/", "value": "disable" }
      ],
      "options":[
        { "key": "/*", "value": "ok" }
      ],
      "all":[
        { "key": "/*", "value": "disable" }
      ]
    }
  },
  "admin":{
    "path":"/admin",
    "adapter":"adminAdapter",
    "route": {
      "get": [
        { "key": "/list", "value": "listObjs" },
        { "key": "/ls", "value": "listObjs" },
        { "key": "/setup", "value": "setup" }
      ]
    }
  },
  "settings":{
    "json":{
      "pretty":true
    }
  }
}
